/* Exemplo de Programa na linguagem L */

int n:=1;
string nome;
char letra;
const MAXITER=10;

write("Digite seu nome: ");
readln(nome);

writeln("Inicial: ", nome[0]);

writeln("");

writeln("Altere a primeira letra do seu nome: ");
write("> ");
readln(letra);

writeln("");

nome[0] := letra;

writeln("Nome alterado: ", nome);